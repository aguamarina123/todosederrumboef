<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/img_navigation"
    app:startDestination="@id/conFragment">

    <fragment
        android:id="@+id/conFragment"
        android:name="com.enzo.evaluacionfatality.Fragments.ConFragment"
        android:label="fragment_con"
        tools:layout="@layout/fragment_con" >
        <action
            android:id="@+id/action_conFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/favFragment"
        android:name="com.enzo.evaluacionfatality.Fragments.FavFragment"
        android:label="fragment_fav"
        tools:layout="@layout/fragment_fav" >
        <action
            android:id="@+id/action_favFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/infoFragment"
        android:name="com.enzo.evaluacionfatality.Fragments.InfoFragment"
        android:label="fragment_info"
        tools:layout="@layout/fragment_info" >
        <action
            android:id="@+id/action_infoFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/salirFragment"
        android:name="com.enzo.evaluacionfatality.Fragments.SalirFragment"
        android:label="fragment_salir"
        tools:layout="@layout/fragment_salir" >
        <action
            android:id="@+id/action_salirFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.enzo.evaluacionfatality.Fragments.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" />
</navigation>